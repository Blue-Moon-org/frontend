{
  "expo": {
    "name": "Bluemoon",
    "slug": "Bluemoon",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#F19319"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.app.bluemoon",
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff",
        "googleServicesFile": "./google-services.json"
      },
      "plugins": [
        "@react-native-google-signin/google-signin",
        [
          "expo-image-picker",
          {
            "photosPermission": "Allow Blue moon to access your photos."
          },
          "expo-camera",
          {
            "cameraPermission": "Allow Blue moon to access your camera."
          },
          {
            "locationAlwaysAndWhenInUsePermission": "Allow Blue moon use your location."
          }
        ]
      ],
      "package": "com.app.bluemoon"
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "scheme": "Bluemoon",
    "extra": {
      "eas": {
        "projectId": "7caece85-318e-479f-9422-c93a37833f20"
      }
    }
  }
}
